  <style>
    /* Keep the button and dropdown inline */
    li.login-btn {
      position: relative;
      display: inline-block;
    }

    .hover-image {
      cursor: pointer;
      transition: transform 0.3s ease-in-out;
    }

    .hover-image:hover {
      transform: scale(1.1);
    }

    /* Message-style box */
    .message-box {
      display: none;
      position: absolute;
      top: 100%;
      left: 50%;
      transform: translateX(-50%);
      background-color: #f9f9f9;
      box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
      border-radius: 10px;
      padding: 15px;
      z-index: 1000;
      width: 200px;
      text-align: left;
      opacity: 0;
      visibility: hidden;
      transition: all 0.3s ease;
    }

    /* Message-box item styles */
    .message-box-item {
      display: flex;
      align-items: center;
      padding: 10px 0;
      transition: background-color 0.3s ease;
    }

    .message-box-item a {
      text-decoration: none;
      color: #333;
      margin-left: 10px;
      font-weight: bold;
    }

    .message-box-item i {
      color: #e23900;
      font-size: 20px;
    }

    /* Hover effect for the message box items */
    .message-box-item:hover {
      background-color: #eef1f5;
      border-radius: 5px;
    }

    /* Show the message-box on hover */
    li.login-btn:hover .message-box {
      display: block;
      opacity: 1;
      visibility: visible;
      transition: opacity 0.3s ease-in-out;
    }

    /* zooming style  */
    .product-container {
      position: relative;
      width: 400px;
    }

    .product-large-img img {
      width: 100%;
      height: auto;
    }

    #zoom-lens {
      position: absolute;
      border: 1px solid #d4d4d4;
      width: 100px;
      height: 100px;
      opacity: 0.4;
      background-color: lightgray;
      display: none;
      pointer-events: none;
      z-index: 1000;
    }

    #zoom-result {
      width: 650px;
      height: 650px;
      border: 1px solid #d4d4d4;
      background-repeat: no-repeat;
      background-size: 800px 800px; /* Adjust for zoom level */
      display: none;
      position: absolute;
      top: 0;
      left: 600px; /* Adjust if you need it closer/further from the product image */
      overflow: hidden; /* Ensure the zoomed image doesnâ€™t exceed this area */
      z-index: 1000;
    }

    .header-right ul {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  list-style-type: none;
  margin: 0;
  padding: 0;
}

.login-btn {
  display: flex;
  align-items: center;
  position: relative;
}

.login-btn a {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50px;
  height: 25px;
  position: relative;
}

.login-btn img.hover-image {
  /* width: 24px; */
  /* height: 24px; */
  object-fit: contain;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.message-box {
  position: absolute;
  top: calc(100% + 0px);
  right: 0;
  background-color: #fff;
  border-radius: 8px;
  box-shadow: 0 2px 15px rgba(0, 0, 0, 0.1);
  /* min-width: 160px; */
  /* padding: 20px 0; */
  z-index: 1000;
  opacity: 1;
  visibility: hidden;
  transition: opacity 0.2s, visibility 0.2s;
}

.login-btn:hover .message-box,
.message-box:hover {
  opacity: 1;
  visibility: visible;
}

.message-box-item {
  display: flex;
  align-items: center;
  /* padding: 3px 0px; */
  transition: background-color 0.2s ease;
}

.message-box-item:hover {
  background-color: rgba(0, 0, 0, 0.05);
}

.message-box-item i {
  width: 20px;
  margin-right: 12px;
  color: #666;
}

.message-box-item a {
  color: #333;
  text-decoration: none;
  font-size: 14px;
  font-weight: 500;
  display: block;
  width: 100%;
}

.message-box::before {
  content: '';
  position: absolute;
  top: -6px;
  right: 80px;
  width: 13px;
  height: 12px;
  background-color: #fff;
  transform: rotate(45deg);
  box-shadow: -2px -2px 5px rgba(0, 0, 0, 0.05);
}

.message-box-item i {
  color: #e23900;
}

